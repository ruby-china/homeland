<% title_tag 'Markdown 教程' %>

<% cache(['markdown-guide', Digest::MD5.hexdigest(Homeland::Markdown.example)]) do %>
<div class="card markdown">
  <div class="card-body">
    <h1>Markdown 教程</h1>
    <div class="row">
      <div class="col-md-6">
        <pre><code><%= Homeland::Markdown.example %></code></pre>
      </div>
      <div class="col-md-6">
        <%= markdown(Homeland::Markdown.example)%>
      </div>
    </div>
  </div>
</div>
<% end %>
