<% title_tag 'Markdown 教程' %>

<% cache(['markdown-guide', Digest::MD5.hexdigest(RubyChina::Markdown.example)]) do %>
<div class="panel markdown panel-default">
  <div class="panel-body">
    <h1>Markdown 教程</h1>
    <div class="row">
      <div class="col-md-6">
        <pre><code><%= RubyChina::Markdown.example %></code></pre>
      </div>
      <div class="col-md-6">
        <%= markdown(RubyChina::Markdown.example)%>
      </div>
    </div>
  </div>
</div>
<% end %>
