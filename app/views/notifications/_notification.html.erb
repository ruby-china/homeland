<%= cache(["notifications",notification]) do %>
<tr id="<%= notification.anchor %>">
  <%= render :partial => "notifications/#{notification.class.name.underscore}", :locals => {:notification => notification} %>
  <td class="last">
    <%= link_to t("common.delete"), notification_path(notification), :method => :delete, :remote => true, :class => 'label delete label-default' %>
  </td>
</tr>
<% end %>
